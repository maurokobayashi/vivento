{"ts":1363121298031,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"#encoding: utf-8\nmodule FacebookHelper\n\n    require 'koala'\n\n    def fb_user(fb_id, fileds)\n        new_graph\n        @graph.get_object(fb_id, :fields => fileds.join(', '))\n    end\n\n    def fb_users(fb_ids)\n        new_graph\n        @graph.get_objects(fb_ids)\n    end\n\n    def fb_picture(fb_id, size=200)\n        new_graph\n        @graph.get_picture(fb_id, :width => size, :height => size)\n    end\n\n    def fb_likes(fb_id)\n        new_graph_token\n        @graph.get_connections(fb_id, \"likes\")\n    end\n\n    private\n        def new_graph\n            @graph = Koala::Facebook::API.new\n        end\n\n    private\n        def new_graph_token\n            @graph = Koala::Facebook::API.new(Facebook::TOKEN.to_s)\n        end\n\nend"]],"start1":0,"start2":0,"length1":0,"length2":720}]],"length":720}
